{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "strict": false,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["./deno.d.ts"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}

